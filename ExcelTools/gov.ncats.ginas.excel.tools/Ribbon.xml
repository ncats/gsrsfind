<?xml version="1.0" encoding="UTF-8"?>
<customUI onLoad="Ribbon_Load" xmlns="http://schemas.microsoft.com/office/2009/07/customui">
    <ribbon>
        <tabs>
            <tab id="ginasTab" label="g-srs">
                <group id="retrievalgroup" label="Retrieval">
                    <button id="button1" onAction="button1_Click" label=" Get Data" />
                    <button id="button2" onAction="button2_Click" label=" Get Structure(s)" />
                </group>
                <group id="updateGroup" label="Update">
                    <button id="button3" onAction="button3_Click_1" label=" Load Data" />
                    <button id="button4" onAction="button4_Click" label=" Create Loading Sheet" />
                </group>
                <group id="groupSDFile" label="SD File">
                    <button id="buttonSdFileImport" onAction="buttonSdFileImport_Click" supertip="Read a structure data format file into the current sheet" label="Import SD File" />
                    <button id="buttonSelectPT" onAction="buttonSelectPT_Click" supertip="Mark a column as Preferred Term for substance creation" label="Select PT" showImage="false" />
                    <button id="buttonAssureColumns" onAction="buttonAssureColumns_Click" label="Assure Required Columns" showImage="false" />
                </group>
                <group id="configurationGroup" label="Configuration">
                    <button id="buttonConfigure" onAction="buttonConfigure_Click" label=" Configure" />
                    <button id="buttonAbout" onAction="buttonAbout_Click" label="About.." showImage="false" />
                </group>
            </tab>
        </tabs>
    </ribbon>
  <contextMenus>
    <contextMenu idMso="ContextMenuFolder">
      <button idMso="FolderPropertiesContext" label="Select Column" onAction="MenuAction" />
    </contextMenu>
  </contextMenus>
</customUI>